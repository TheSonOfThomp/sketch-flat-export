{
  "name": "rename-export",
  "version": "2.0.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "rename-export",
    "manifest": "src/manifest.json",
    "main": "rename-export.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "Adam Thompson <athompson@newvisions.org>",
  "repository": "https://github.com/TheSonOfThomp/Batch-Export-Rename",
  "description": "Use this plugin to batch export layers with consistent filenames"
}
